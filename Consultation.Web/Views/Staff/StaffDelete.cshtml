@model Staff

<partial name="_Breadcrumbs" model=@(new [] { ("/", "Home"), ("/staff", "Staffs"), ("","Delete") })>

    <h1>Delete</h1>

    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>Patient</h4>
        <hr />
        <dl class="row">
            <dt class="col-5">User.Name</dt>
            <dd class="col-7">@Model.User.Name</dd>

            <dt class="col-5">Position</dt>
            <dd class="col-7">@Model.Position</dd>

            <dt class="col-5">User.Email</dt>
            <dd class="col-7">@Model.User.Email</dd>

            <dt class="col-5">Mobile</dt>
            <dd class="col-7">@Model.Mobile</dd>

        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index">Back to List</a>
        </form>
    </div>
